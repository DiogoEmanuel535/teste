programa {
  funcao inicio() {
    //Variaveis
    inteiro x,y,troca

    //Entrada
    escreva("Insirar o x ")
    leia(x)
    escreva("Insirar o y ")
    leia(y)

    //processamento

    troca=x
    x=y
    y=troca

    //saida

    escreva("valor de x ", x)
    escreva("valor de y ", y )


  }
}
